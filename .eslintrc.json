{
  "parser": "babel-eslint",
  "globals": {
    "Promise": true,
    "Uint8Array": true
  },
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "keyword-spacing": 2,
    "no-extra-semi": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-var": 2,
    "prettier/prettier": [2, {
      "trailingComma": "none",
      "printWidth": 100,
      "singleQuote": true
    }],
    "semi": [
      2,
      "always"
    ]
  }
}
